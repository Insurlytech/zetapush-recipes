/**
 *
 */
macroscript setTags(
	@NotNull string id,
	@NotNull array tags
) {
	/** Store tags */
	var response = call setStoredObject({
		id, value: usr:reduce_array_to_map(tags), table: TABLE_TAGS, zpService: zpServiceGda
	}) hardFail;
	/** Destructure result properties */
	var value = usr:reduce_map_to_array(response.result.value);
	var revision = response.result.revision;
	var update = response.result.update;
} broadcast { id, value, revision, update } on channel __selfName
