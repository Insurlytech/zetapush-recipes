macroscript deleteRole(
	@NotNull string name
) {
	var id = str:hex(str:md5(str:upper(name)));
	/** Delete role based group */
	sudo zpRecipeUtils::GLOBAL_OWNER call zpRecipeGroup::deleteGroupByService({
		id,
		zpService: zpServiceGroupsRole
	}) hardFail;
} broadcast { id, name } on channel __selfName
