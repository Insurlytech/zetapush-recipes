/**
 *	Delete a room with his ID
 */
macroscript deleteRoom(
	/** Room id */ @NotNull string id
) {
	const { room } = await core_room__deleteRoomByService({
		id : id, 
		zpService : zpServiceGroups
	});
	
	sudo zpRecipeUtils::GLOBAL_OWNER zpServiceGda.removeRow({
		table : TABLE_UNREAD_MESSAGES,
		key : id
	});
	
} on channel __selfName
