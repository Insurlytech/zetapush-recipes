/**
 *
 */
macroscript deleteFileEntry(
	/** */ @NotNull string path,
	/** */ string owner
) {
	/** Manage default parameters */
	owner = owner ?: __userKey;
	trace(#deleteFileEntry, path, owner);
	/** Remove file */
	sudo owner zpServiceFile.rm({
		path,
		owner
	});
} broadcast {
	string path,
	string owner
} on channel __selfName
