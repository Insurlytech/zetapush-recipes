/**
 *
 */
macroscript requestFileUpload(
	@NotNull string contentType,
	@NotNull string folder
) {
	trace(#requestFileUpload, contentType, folder);
	/** Request file upload */
	var { result: { httpMethod, guid, url, owner} } = call core_filesystem__requestFileUploadByService({	contentType, folder, zpService: zpServiceFileSystem }) hardFail;
} broadcast { 
	httpMethod, guid, url, owner
} on channel __selfName
