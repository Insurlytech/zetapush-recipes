/**
 *
 */
macroscript getFileEntryList(
	@NotNull string folder
) {
	trace(#getFileEntryList, folder);
	/** Get file upload list */
	var { result: { exists, entries, owner } } = call core_filesystem__getFileEntryListByService({ folder, zpService: zpServiceFileSystem }) hardFail;
} return { 
	exists, folder, entries, owner
} on channel __selfName
