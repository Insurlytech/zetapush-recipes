/** 
 *	Macroscript called when the call is accepted.
 *	We change the state of the call in the database
 */
macroscript setCallAccepted(
	/** ID of the call */ @NotNull string id
) {
	// Change the state of the call
	call setCallState({id : id, state : STATE_CALL_ACCEPTED});

} return { id } on channel __selfName