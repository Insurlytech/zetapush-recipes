macroscript getRoleList(
) {
	/** Get group list from API */
	var list = sudo zpRecipeUtils::GLOBAL_OWNER zpServiceGroupsRole.allGroups({});
	/** Delegate get all user groups */
	var { result: { groups } } = sudo zpRecipeUtils::GLOBAL_OWNER call zpRecipeGroup::core_group__listGroupByService({
		list,
		zpService: zpServiceGroupsRole
	}) hardFail;
} return {
	list: groups
} on channel __selfName
