 */
macroscript core_conversation__addConversationMessageCallback(
	@NotNull string container,
	@ValidConversationRoom room,
	@NotNull map message
) {
	/** Get pascalified message type */
	var type = usr:core_conversation__pascalify(message.type);
	/** Get computed macro name */
	var name = 'core_conversation__addConversation' + type + 'MessageCallback';
	/** Dynamic conversation message callback */
	zpServiceImplMacro.func({
		parameters: __parameters,
		name
	});
} return {
	
} on channel __selfName

