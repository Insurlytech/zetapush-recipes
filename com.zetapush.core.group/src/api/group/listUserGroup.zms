/**
 * Get all the groups user is part of
 */
macroscript listUserGroup(
	
) {
	/** Get group list from API */
	var list = zpServiceGroups.myGroups({});
	/** Delegate get all my groups */
	var { result: { groups } } = call core_group__listGroupByService({ list, zpService: zpServiceGroups }) hardFail;
} broadcast { groups } on channel __selfName
